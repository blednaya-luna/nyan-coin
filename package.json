    "@eslint-kit/eslint-config-typescript": "^5.0.1",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/github": "^8.0.1",
    "@semantic-release/npm": "^8.0.2",
    "@types/history": "^4.7.8",
    "@types/jest": "^26.0.9",
    "@types/react": "^16.9.46",
